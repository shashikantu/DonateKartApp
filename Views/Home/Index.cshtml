@{
    ViewData["Title"] = "Home Page";
   @model List<ProductModel>
}

<div class="text-center">
    <div class="col-md-6" style="float:left ;width:25% ;height:50px">
        <img src="~/image/ball.jfif"/>
        <div>
            <label>ball</label>
            <label id="ball"></label>
        </div>
        
    </div>
    <div class="col-md-6" style="float: left; width: 25%; height: 50px">
        <img src="~/image/bat.jfif" />
        <div>
            <label>bat</label>
            <label id="bat" ></label>
        </div>
    </div>
    <div class="col-md-6" style="float: left; width: 25%; height: 50px">
        <img src="~/image/shoes.jfif" />
        <div>
            <label>shoes</label>
            <label id="shoes"></label>
        </div>
    </div>
    <div class="col-md-6" style="float: left; width: 25%; height: 50px">
        <label for="cars"> Currency:</label>
        <select name="cars" id="currency">
            <option value="INR">INR</option>
            <option value="USD">USD</option>
        </select>
    </div>
</div>
